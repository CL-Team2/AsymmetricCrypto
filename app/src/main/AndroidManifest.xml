<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asymmetriccrypto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Encrypt"
            android:label="Encrypt a Message" />
        <activity
            android:name=".ShowPublicKey"
            android:label="Your Public Key" />
        <activity
            android:name=".Decrypt"
            android:label="Decrypt a Message" />
        <activity
            android:name=".EncryptedMessage"
            android:label="Your Encrypted Message" />
        <activity android:name=".DecryptedMessage"
            android:label="Your Decrpted Message"></activity>
    </application>

</manifest>